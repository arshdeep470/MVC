    <script src="~/js/jquery.dataTables.min.js"></script>
    <script src="~/js/jquery-ui.min.js"></script>
    <script>
        var timeoutWarning = @TimeOutWarningInMilliSeconds;
        var timeoutNow = @TimeOutInMilliSeconds;
        var logoutUrl = '/Home/Logout?path=SessionTimeout'

        var warningTimer;
        var timeoutTimer;

        function idleTimeOut() {
            $("#TimeOutDialogBox").dialog("close");
            location.href = logoutUrl;
        }

        function showAutoSignoutWarning() {
            $("#TimeOutDialogBox").dialog("open");
        }

        function startTimer() {
            timeoutTimer = setTimeout("idleTimeOut()", timeoutNow);
            warningTimer = setTimeout("showAutoSignoutWarning()", timeoutWarning);
        }

        function resetTimer() {
            clearTimeout(timeoutTimer);
            clearTimeout(warningTimer);
            startTimer();
            $("#TimeOutDialogBox").dialog("close");
        }


        $(document).ready(function () {
            if ($("#isUserLoggedIn").val().toLowerCase() == "true") {
                $("#TimeOutDialogBox").removeAttr('hidden');
                startTimer();
                $("body").on('click keypress', function () {
                    resetTimer();
                });
                $("#TimeOutDialogBox").dialog({
                    autoOpen: false,
                    height: 250,
                    width: 450
                });
            }

            CheckBrowserCompatibility();
        });



    </script>
</head>
<body>
    @{
        bool showOutageWarning = Convert.ToBoolean(Environment.GetEnvironmentVariable("SHOW_OUTAGE_WARNING"));
        string ShieldOutageWarning = Environment.GetEnvironmentVariable("SHIELD_OUTAGE_WARNING");
        string SessionTimeoutMessage = Environment.GetEnvironmentVariable("UserSessionTimeOutMessage");
        var showManageLine = false;
        var showAddUser = false;
        var canResetPin = false;
        var showSessionTimeoutMessage = false;
        var currentUserString = Context.Request.Cookies["shield_current_user"];
        int currentUserBemsId = 0;
        var userLoggedIn = false;
        bool isErrorPage = string.Equals(Context.Request.Path.Value, "/Error", StringComparison.InvariantCultureIgnoreCase);
        if (currentUserString != null)
        {
            var currentUser = JsonConvert.DeserializeObject<User>(currentUserString);
            currentUserBemsId = currentUser.BemsId;
            var userHasARole = currentUser != null &&
            currentUser.Role != null &&
            currentUser.Role.Name != null;
            showManageLine = userHasARole && (currentUser.IsGC() || currentUser.IsBC() || currentUser.IsSystemAdmin() || currentUser.IsSiteAdmin());
            showAddUser = userHasARole && (currentUser.IsManager || currentUser.IsSystemAdmin() || currentUser.IsSiteAdmin() || currentUser.IsEngineer());
            canResetPin = userHasARole && (currentUser.IsGC() || currentUser.IsBC() || currentUser.IsSystemAdmin() || currentUser.IsSiteAdmin() || currentUser.IsCCAndPAE());
            userLoggedIn = userHasARole;
        }

        if (TempData["SessionTimeout"] != null && !String.IsNullOrEmpty(TempData["SessionTimeout"].ToString()) && String.Equals(TempData["SessionTimeout"].ToString(), "true", StringComparison.CurrentCultureIgnoreCase))
        {
            TempData.Remove("SessionTimeout");
            showSessionTimeoutMessage = true;
            userLoggedIn = false;
        }

        var pageTitle1 = "";
        if (ViewData["Title"] == null || ViewData["Title"].ToString().Trim().Equals(""))
        {
            pageTitle1 = "SHIELD";
        }
        else
        {
            pageTitle1 = @ViewData["Title"] + " | SHIELD";
        }
        var backupUI = Environment.GetEnvironmentVariable("BACKUP_UI_URL");
        String autoLogoutWarningMessage = Environment.GetEnvironmentVariable("LogOutWarningMessage");

        string environmentMessage = Environment.GetEnvironmentVariable("EnvironmentMessage");
        string checkInHelpLink = Environment.GetEnvironmentVariable("CheckInHelpLink");
        string userGuideLink = Environment.GetEnvironmentVariable("UserGuideLink");
        string releaseNotes = Environment.GetEnvironmentVariable("ReleaseNotes");
    }
