            pageTitle = "SHIELD";
        }
        else
        {
            pageTitle = @ViewData["Title"] + " | SHIELD";
        }

        //var backupUI = Environment.GetEnvironmentVariable("BACKUP_UI_URL");
        int TimeOutInMilliSeconds = Convert.ToInt32(Environment.GetEnvironmentVariable("UserSessionTimeOutInMilliSeconds"));
        int TimeOutWarningInMilliSeconds = Convert.ToInt32(Environment.GetEnvironmentVariable("UserSessionTimeOutWarningInMilliSeconds"));
