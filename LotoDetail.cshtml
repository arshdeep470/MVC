@*Popup for Tranfer comments*@
<div class="modal fade" id="transferCommentModal" tabindex="-1" role="dialog" aria-labelledby="TransferCommentPopupLabel" aria-hidden="true">
	<div class="modal-dialog modal-md" role="document">
		<div class="modal-content">
			<div class="modal-header blue darken-1 white-text">
				<h4 class="modal-title">Transfer LOTO</h4>
				<button type="button" class="close text-white select-close" data-dismiss="modal" aria-label="Close" onclick="clearTransferComment()">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<form method="post" id="transfer-loto-form"
				  asp-controller="Loto"
				  asp-action="Transfer"
				  asp-route-lotoId="@Model.Loto.Id"
				  asp-route-lineNumber="@Model.Loto.LineNumber"
				  asp-route-program="@Model.Loto.Program"
				  asp-route-site="@Model.Header.Aircraft.Site">
				<div id="TransferPopupModalBody">
					<div class="modal-body" style="padding: 0rem 3.5rem 0 3.5rem; margin-top:1.2rem">
						<div id="transfer-comment-area">
							<input type="text" style="border-color:black" oninput="validateTransferComments()" class="form-control"
								   id="transfer-comments" name="comment" placeholder="Enter the reason for Transfer..." />
						</div>
					</div>
					<div class="modal-footer" id="transfer-cancel-button">
						<button type="button" class="btn btn-secondary-gray btn-rounded black-text float-left" value="Cancel" onclick="clearTransferComment()" data-dismiss="modal">
							<i class="fa fa-times prefix" style="padding-right:.4rem; color:black !important; margin-top:-.19rem;"></i>
