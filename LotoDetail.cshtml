											<div class="card tab-pane fade" id="sign-in-visitor" role="tabpanel" aria-labelledby="sign-in-visitor-tab" style="border-top-left-radius: 0;">
												<div id="visitor-sign-in-partial" class="card-body d-flex flex-column" style="margin-top:0%;">
													@Html.Partial("Partials/LotoVisitorSignInCard", new Shield.Ui.App.ViewModels.LotoAEManagementViewModel
													{
													Header = Model.Header,
													Loto = Model.Loto,
													SessionService = Model.SessionService,
													Message = Model.SignInCardMessage
													})
												</div>
											</div>
										</div>
									</div>

								</div>
								@Html.Partial("Partials/LotoSignOutCard", new Shield.Ui.App.ViewModels.LotoAEManagementViewModel
								{
								Header = Model.Header,
								Loto = Model.Loto,
								SessionService = Model.SessionService
								})
							</div>
							<div id="info-card" class="card" style="margin-top:20px">
								<div id="loto-job-info-wrapper" class="card-body">
									@if (!isLotoFromDiscrete)
									{
										@Html.Hidden("Site", Model.Header.Aircraft.Site)
										@await Html.PartialAsync("Partials/LotoJobInfoPartial", Model.Loto)
									}
									else
									{
										@Html.Hidden("Id", Model.Loto.Id)
										@Html.Hidden("AssignedPAEBemsId", Model.Loto.AssignedPAE == null ? (int?)null : Model.Loto.AssignedPAE.BemsId)
										@await Html.PartialAsync("Partials/LotoWithDiscreteJobInfoPartial", Model.Loto.Discrete)
									}
								</div>
							</div>

							@if (isLotoFromDiscrete || Model.Loto.HasValidJobInfoData)
							{
								<div id="isolations-wrapper" style="margin-top:10px">
									<div>
										<ul class="nav" style="margin-bottom:3px;">
											<li class="loto-tab" style=""><a id="isolations-tab" data-toggle="tab" data_id="1" class="nav-link loto-tab-link active" href="#isolations">Isolations</a></li>
											<li class="loto-tab" style="width:12rem;"><a data-toggle="tab" class="nav-link loto-tab-link" data_id="2" href="#loto-history">LOTO History Log</a></li>
										</ul>
										<div class="card">
											<div class="card-body">
												<div class="tab-content loto-tab-content">
													<div id="isolations" class="tab-pane active fade in show">
														@if (isLotoFromDiscrete && Model.Loto.IsNeedsLockOut())
														{
															@await Html.PartialAsync("Partials/LotoWithDiscreteIsolationsPartial", Model.Loto)
														}
														else if (Model.Loto.IsNeedsLockOut())
														{
															@await Html.PartialAsync("Partials/LotoIsolationsPartial", Model.Loto)
														}
														else if (Model.Loto.IsActive() || Model.Loto.IsTransfer())
														{
															@await Html.PartialAsync("Partials/ActiveLotoIsolationsPartial", Model.Loto)
														}
														else if (Model.Loto.IsComplete())
														{
															@await Html.PartialAsync("Partials/CompleteLotoIsolationsPartial", Model.Loto)
														}
													</div>
													<div id="loto-history" class="tab-pane fade">
														<div id="lotoHistoryTable">
															@await Html.PartialAsync("Partials/LotoHistoryTable", Model.LotoTransactions)
														</div>
													</div>
												</div>
											</div>
										</div>

										<br />

									</div>
								</div>
							}
							<div>
								<div class="row">
									<div class="col-md-5">
										<a id="back-button" class="btn btn-secondary-gray btn-rounded float-left"
										   asp-controller="Loto" asp-action="ViewLotos"
										   asp-route-site="@Model.Header.Aircraft.Site"
										   asp-route-program="@Model.Header.Aircraft.Model"
										   asp-route-lineNumber="@Model.Header.Aircraft.LineNumber"
										   style="position: relative; margin-left:0">
											<i class="fa fa-arrow-left prefix" style="padding-right:5px"></i>
											Back
										</a>
									</div>
									<div class="col-md-2 text-center">
										<button id="save-for-later-link" onclick="printWindow()" class="btn btn-link" style="color: #0275d8 !important; text-decoration: underline; text-transform: none !important; font-size:.85rem">Print</button>
									</div>

									<div class="col-md-5">
										@if (Model.Loto.Status.Description == Shield.Common.Constants.Status.NEEDS_LOCKOUT_DESCRIPTION)
										{

											<div class="float-right" id="LockoutButtonBlock">
												<button type="submit" @Html.Raw(needsLockoutDisabledAttr) aria-label="Lockout" id="lockout-button" class="btn btn-primary btn-rounded float-right" onclick="getLockOutModal(@Model.Loto.ConflictIsolations.Count);"><i class="fa fa-lock prefix"></i> Lockout</button>
											</div>
										}

										@* Lockout modal *@
										<div class="modal fade" id="lockoutModal" tabindex="-1" role="dialog" aria-hidden="true">
											<div class="modal-dialog modal-md" role="document">
												<div class="modal-content">
													<div class="modal-header warning-color-dark darken-1 white-text">
														<h3 class="modal-title">Lockout Aircraft?</h3>
														<button type="button" class="close" data-dismiss="modal" aria-label="Close">
															<span aria-hidden="true">&times;</span>
														</button>
													</div>
													<div class="modal-body" id="lockout-modal-body">
														<p>
															Press 'LOCKOUT' if the Tags/Locks have already been installed and the <strong>aircraft is safe to work on</strong>.
														</p>
														<p>
															Press 'CANCEL' if the Tags/Locks still need to be installed. Select the 'Lockout' button again when they have been installed and the aircraft is locked out and safe to work on.
														</p>
													</div>
													<div id="confirming-author-modal-actions" class="modal-footer">
														<button id="cancel-lockout" type="button" class="btn btn-rounded btn-secondary-gray black-text float-right" data-dismiss="modal">Cancel</button>
														<form asp-controller="Loto" asp-action="Lockout" asp-route-lotoId="@Model.Loto.Id" asp-route-lineNumber="@Model.Loto.LineNumber" asp-route-program="@Model.Loto.Program" asp-route-site="@Model.Header.Aircraft.Site">
															<button type="submit" aria-label="Lockout" id="lockout-modal-button" class="btn btn-rounded btn-warning black-text float-right"><i class="fa fa-lock prefix"></i>LOCKOUT</button>
														</form>
													</div>
												</div>
											</div>
										</div>

										@* Conflict Isolation modal *@
										<div class="modal fade" id="conflictLockoutModal" tabindex="-1" role="dialog" aria-hidden="true">
											<div class="modal-dialog modal-md" role="document">
												<div class="modal-content">
													<div class="modal-header info-color-dark darken-1 white-text">
														<h3 class="modal-title">Lockout Conflict</h3>
														<button type="button" class="close" data-dismiss="modal" aria-label="Close">
															<span aria-hidden="true">&times;</span>
														</button>
													</div>
													<div class="modal-body" id="lockout-modal-body">
														@{
															var conflictedLotoName = string.Empty; int rowCount = 1;
														}

														@foreach (var iso in Model.Loto.ConflictIsolations)
														{

															if (!conflictedLotoName.Equals(iso.ConflictLotoName))
															{
																<p>The LOTO for work package <strong>@Model.Loto.WorkPackage</strong> cannot be applied because the LOTO for work package <strong>@iso.ConflictLotoName</strong> has conflicting component isolation states.</p>
																<br />
																<br />
																<label>Conflicts with :</label>
																<table class="table table-small" id="active-loto-isolation-table">
																	<thead>
																		<tr>
																			<th width="10%">ID</th>
																			<th width="25%" class="">Name</th>
																			<th width="10%" class="">Set to</th>
																		</tr>
																	</thead>
																	<tbody>
																		@foreach (var conflt in Model.Loto.ConflictIsolations)
																		{
																			if (conflt.ConflictLotoId == rowCount)
																			{
																				<tr>
																					<td>@conflt.CircuitId</td>
																					<td>@conflt.CircuitName</td>
																					<td>@conflt.State</td>
																				</tr>
																			}
																		}
																	</tbody>
																</table>

																conflictedLotoName = @iso.ConflictLotoName;
																rowCount = rowCount + 1;
															}

														}
													</div>

													<div id="confirming-author-modal-actions" class="modal-footer">
														<button id="cancel-lockout" type="button" class="btn btn-rounded btn-secondary-gray black-text float-right" data-dismiss="modal">OK</button>
													</div>
												</div>
											</div>
										</div>

										<div class="modal fade" id="conflictIsolationsModal" tabindex="-1" role="dialog" aria-hidden="true">
										</div>

										@if (Model.Loto.IsActive())
										{
											<div class="float-right">
												<button type="button" aria-label="Transfer" id="transfer-button" class="btn btn-primary btn-rounded"
												@disableTransferAttr onclick="showTransferDialog()">
													Transfer
													<img width="25" height="25" class="img-responsive" id="sign-in-loader" style="display: none;" alt="Loading..." src="/images/loading.gif">
												</button>
											</div>
											<div class="float-right">
												<button id="complete-button"
														class="btn btn-primary btn-rounded"
														type="button"
														data-target="#completeModal"
														data-toggle="modal"
												@disableCompleteAttr
												@Html.Raw(completeButtonDisabledAttr)>
													Remove Isolations & Complete
												</button>
											</div>
										}
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>


<div class="modal fade" id="completeModal" role="dialog">
	<div class="modal-dialog modal-md" role="document">
		<div class="modal-content">
			<div class="modal-header modal-header-primary white-text">
				<h3 class="modal-title">Complete LOTO</h3>
			</div>
			<div class="modal-body">
				<div style="margin-top:1.5rem; margin-bottom:2rem;">
