@using Shield.Ui.App.Models.LotoModels

@model Shield.Ui.App.ViewModels.LotoDetailViewModel
@{
	var currentUser = Model.SessionService.GetUserFromSession(Context);
	var isLotoFromDiscrete = Model.Loto.Discrete != null;
	var needsLockoutDisabledAttr = "";

	if (Model.Loto.IsNeedsLockOut())
	{
		List<LotoIsolationsDiscreteHecp> discreteIsolations = Model.Loto.LotoAssociatedHecps.SelectMany(x => x.LotoIsolationsDiscreteHecp).ToList();

		if (isLotoFromDiscrete)
		{
			List<Shield.Common.Models.Loto.Shared.Isolation> isolations = Model.Loto.Discrete.DiscreteDeactivationSteps
				.Where(step => step.Isolations?.Count > 0)
				.SelectMany(step => step.Isolations)
				.ToList();

			var unlockedIsolations = isolations.Where(iso => !iso.IsLocked).ToList();

			if (unlockedIsolations.Count > 0 || isolations == null)
			{
				needsLockoutDisabledAttr = "disabled";
			}
		}
		if (Model.Loto.LotoAssociatedHecps.Count > 0)
		{
			if (Model.Loto.HecpIsolationTags != null && Model.Loto.HecpIsolationTags.Count > 0)
			{
				var totalIsolations = Model.Loto.HecpIsolationTags.ToList();
				var unlockedIsolations = Model.Loto.HecpIsolationTags.Where(iso => iso.IsLocked && iso.Id > 0).ToList();
				if (unlockedIsolations.Count < totalIsolations.Count)
				{
					needsLockoutDisabledAttr = "disabled title=\"No isolations have been added to this LOTO.\"";
				}
			}
		}
		if (Model.Loto.HecpIsolationTags.Count == 0 && discreteIsolations.Count == 0) // DISABLE needs lockout button if there are no isolations, or if the current user doesn't have permission to lockout the LOTO
		{
			needsLockoutDisabledAttr = "disabled title=\"No isolations have been added to this LOTO.\"";
		}
		else if (Model.Loto.AssignedPAE?.BemsId != currentUser?.BemsId)
		{
			needsLockoutDisabledAttr = "disabled title=\"Only the assigned PAE can lockout a LOTO.\"";
		}

		if (Model.Header.Aircraft.AssignedGroupCoordinatorBems == 0 || Model.Header.Aircraft.AssignedGroupCoordinatorBems == null)
		{
			needsLockoutDisabledAttr = "disabled title=\"A GC needs to claim the line to perform lockout\"";
		}

	}

	bool disableActionButton = false;
	if (Model.Loto.AssignedPAE != null && Model.Loto.AssignedPAE.BemsId != currentUser.BemsId)
	{
		disableActionButton = true;
	}

	bool disableTransfer = disableActionButton || !Model.Loto.ShowTransfer();
	string disableTransferAttr = "";
	if (disableTransfer)
	{
		disableTransferAttr = "disabled";
	}

	string completeButtonDisabledAttr = "";
	if (Model.Loto.TheAircraftGCBems == 0)
	{
		completeButtonDisabledAttr = "disabled title=\"A GC must claim responsibility for this line and you must be the assigned PAE before the LOTO can be completed\"";
	}

	bool disableComplete = disableActionButton || !Model.Loto.ShowComplete();
	string disableCompleteAttr = "";
	if (disableComplete)
	{
		disableCompleteAttr = "disabled";
	}
}

@*Take care of isolation lockout logic*@
@if (Model.Loto.Discrete != null)
{
	int discreteIsolationCount = Model.Loto.Discrete.DiscreteDeactivationSteps
		.Where(step => step.Isolations?.Count > 0)
		.SelectMany(step => step.Isolations)
		.ToList().Count;

	int discreteInitialIsolationLockedCount = Model.Loto.Discrete.DiscreteDeactivationSteps
		.Where(step => step.Isolations?.Count > 0)
		.SelectMany(step => step.Isolations)
		.Where(isolation => isolation.IsLocked)
		.ToList().Count;

	if (discreteIsolationCount == discreteInitialIsolationLockedCount && !disableActionButton)
	{
		needsLockoutDisabledAttr = "";
	}

	<script type="text/javascript">
		var discreteIsolationTotal = @Html.Raw(discreteIsolationCount);
	</script>
}

